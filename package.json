{
    "name": "@toes/core-monorepo",
    "private": true,
    "version": "0.0.1-dev",
    "description": "The core of the toes",
    "scripts": {
        "prepublishOnly": "pnpm run lint && pnpm run test && pnpm run build",
        "lint": "ultra -r lint",
        "test": "ultra -r test",
        "build": "ultra -r build",
        "publish:all": "pnpm publish --no-git-checks -r",
        "publish:dev": "pnpm publish --no-git-checks -r --tag dev --registry http://npm.firenodes.com/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/throw-out-error/core.git"
    },
    "keywords": [
        "typescript",
        "logger",
        "ts-log",
        "log4js",
        "node-bunyan",
        "dependency-injection",
        "di"
    ],
    "author": "Theo Paris",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/throw-out-error/core/issues"
    },
    "homepage": "https://github.com/throw-out-error/core#README",
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "ultra-runner": "^3.5.0"
    }
}
